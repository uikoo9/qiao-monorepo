{
  "name": "qiao-electron-demo",
  "version": "0.0.0",
  "description": "qiao-electron-cli demo",
  "keywords": [],
  "author": "uikoo9 <uikoo9@qq.com>",
  "scripts": {
    "init-pack": "npm i",
    "init-main": "cd src && npm i",
    "init-main-m1": "cd src && npm i --target_arch=arm64",
    "init-all": "npm run init-pack && npm run init-main",
    "init-all-m1": "npm run init-pack && npm run init-main-m1",
    "start": "electron src",
    "debug": "electron src --inspect=5858",
    "dev": "electron src dev",
    "dist": "qelectron d ./pack/qiao-electronconfig.js",
    "icon": "qelectron icon ./qzip-pack/static/icon/pic.png",
    "prepackmac": "npm run build && npm run dist",
    "packmac": "qelectron pm ./pack/qiao-electronconfig.js",
    "packdmg": "qelectron pd ./pack/qiao-electronconfig.js",
    "uploaddmg": "qelectron ud ./pack/qiao-electronconfig.js"
  },
  "devDependencies": {
    "electron": "^21.1.1",
    "qiao-electron-cli": "^0.8.9"
  }
}
