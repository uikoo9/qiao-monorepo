	"{{className2}}Edit"		: "update {{tableName}} t set {{each params}}t.{{$value.name1}}=?, {{/each}}t.udate=now(), t.uuser_id=?, t.uuser_name=? where t.id=? and t.ucenter_user_id=? and t.del_tag='0'",
	"{{className2}}Del"			: "update {{tableName}} t set t.del_tag='1' where t.ucenter_user_id=? and t.id in (?)",
