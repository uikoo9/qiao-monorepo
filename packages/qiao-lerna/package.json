{
    "name": "qiao-lerna",
    "version": "1.1.8",
    "description": "lerna tools",
    "keywords": [
        "lerna",
        "tools"
    ],
    "author": "uikoo9 <uikoo9@qq.com>",
    "homepage": "https://code.insistime.com/qiao-lerna",
    "license": "MIT",
    "main": "index.js",
    "module": "src/index.js",
    "sideEffets": false,
    "files": [
        "bin",
        "src"
    ],
    "bin": {
        "qlerna": "./bin/qlerna.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uikoo9/qiao-monorepo.git"
    },
    "bugs": {
        "url": "https://github.com/uikoo9/qiao-monorepo/issues"
    },
    "scripts": {
        "build": "rollup --config _config.rollup.js",
        "eslint": "eslint . --ext .js -c _config.eslint.js",
        "eslintfix": "eslint . --ext .js --fix -c _config.eslint.js",
        "prepublish": "npm run build && npm run eslintfix"
    },
    "dependencies": {
        "npm-check-updates": "^16.4.1",
        "qiao-cli": "^1.0.5",
        "qiao-console": "^1.0.0",
        "qiao-file": "^1.0.5",
        "qiao-npms": "^1.0.3",
        "qiao-parallel": "^1.0.6"
    }
}
